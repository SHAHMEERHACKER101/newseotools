    <script>
        // Initialize app for Patreon link functionality
        const patreonUrl = 'https://www.patreon.com/posts/seo-tools-137228615?utm_medium=clipboard_copy&utm_source=copyLink&utm_campaign=postshare_creator&utm_content=join_link';
        
        document.addEventListener('DOMContentLoaded', () => {
            const unlimitedBtn = document.getElementById('unlimited-access-btn');
            const proAccessBtn = document.getElementById('pro-access-btn');
            
            [unlimitedBtn, proAccessBtn].forEach(btn => {
                if (btn) {
                    btn.addEventListener('click', (e) => {
                        e.preventDefault();
                        window.open(patreonUrl, '_blank');
                    });
                }
            });

            // Contact form handling
            const contactForm = document.getElementById('contact-form');
            if (contactForm) {
                contactForm.addEventListener('submit', (e) => {
                    e.preventDefault();
                    // Show success message
                    alert('Thank you for your message! We will get back to you within 24 hours.');
                    contactForm.reset();
                });
            }
        });
    </script>
